# Coding style with EditorConfig
# Plugin for your editor/IDE on http://EditorConfig.org

# top-most EditorConfig file
root = true

# General config
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

# C style
# BSD/Allman
[*.{c,h}]
indent_style = space
indent_size = 4
max_line_length = 120
